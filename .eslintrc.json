{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
        "no-restricted-properties": 0,
        "no-bitwise": 0,
        "no-plusplus": 0,
        "no-mixed-operators": 0,
        "prefer-const": 0,
        "indent": ["error", 4]
    }
}
